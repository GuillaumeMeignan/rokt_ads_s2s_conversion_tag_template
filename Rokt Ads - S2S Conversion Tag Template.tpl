const getAllEventData = require('getAllEventData');
const sendHttpRequest = require('sendHttpRequest');
const JSON = require('JSON');
const Math = require('Math');
const getTimestampMillis = require('getTimestampMillis');
const makeString = require('makeString');
const getType = require('getType');
const logToConsole = require('logToConsole');

const eventData = getAllEventData();

// Build the objectData field
const objectData = [];

// Add common event data
if (eventData.transaction_id) {
  objectData.push({
    "name": "transactionid",
    "value": getType(eventData.transaction_id) === 'string' ? eventData.transaction_id : JSON.stringify(eventData.transaction_id)
  });
}
if (eventData.value) {
  objectData.push({
    "name": "amount",
    "value": getType(eventData.value) === 'string' ? eventData.value : JSON.stringify(eventData.value)
  });
}

// Add personnal data if the user consent
if (data.enhancedConversions) {
  if (data.consent !== "false") {
    if (eventData.user_data.email_address) {
      objectData.push({
        "name": data.emailHashed ? "emailsha256" : "email",
        "value": eventData.user_data.email_address
      });
    }
    if (eventData.user_data.phone_number) {
      objectData.push({
        "name": data.phoneHashed ? "mobilesha256" : "mobile",
        "value": eventData.user_data.phone_number
      });
    }
    if (eventData.user_data.first_name) {
      objectData.push({
        "name": data.firstnameHashed ? "firstnamesha256" : "firstname",
        "value": eventData.user_data.first_name
      });
    }
    if (eventData.user_data.last_name) {
      objectData.push({
        "name": data.lastnameHashed ? "lastnamesha256" : "lastname",
        "value": eventData.user_data.last_name
      });
    }
    if (eventData.user_data.address.street) {
      objectData.push({
        "name": "address1",
        "value": eventData.user_data.address.street
      });
    }
    if (eventData.user_data.address.city) {
      objectData.push({
        "name": "city",
        "value": eventData.user_data.address.city
      });
    }
    if (eventData.user_data.address.region) {
      objectData.push({
        "name": "state",
        "value": eventData.user_data.address.region
      });
    }
    if (eventData.user_data.address.postal_code) {
      objectData.push({
        "name": "zipcode",
        "value": eventData.user_data.address.postal_code
      });
    }
    if (eventData.user_data.address.country) {
      objectData.push({
        "name": "country",
        "value": eventData.user_data.address.country
      });
    }
  }
}

// Add some customer event parameters if necessary
if (data.eventParameters) {
  data.eventParameters.forEach(parameter => {
    if (parameter.value) {
      objectData.push({
        "name": parameter.key,
        "value": getType(parameter.value) === 'string' ? parameter.value : JSON.stringify(parameter.value) 
      });
    }
  });
}

// Build the body request
const postBody = {
  "accountId": data.accountId,
  "events": [
    {
      "clientEventId": eventData.event_id,
      "eventTime": convertTimestampToUTC(getTimestampMillis()),
      "eventType": "conversion",
      "objectData": objectData
    }
  ]
};

// Then send the event request
sendHttpRequest("https://api.rokt.com/v2/events", {
  headers: {
    "Authorization": "Basic " + data.accessToken,
    "Content-Type": "application/json",
    "Charset": "utf-8",
    "Rokt-Version": "2020-05-21"
  },
  method: 'POST',
  timeout: 2000,
}, JSON.stringify(postBody)).then((result) => {
  logToConsole(result);
});

data.gtmOnSuccess();

function convertTimestampToUTC(timestamp) {
  const leapYear = [31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31];
  const nonLeapYear = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31];

  const secToMs = (s) => s * 1000;
  const minToMs = (m) => m * secToMs(60);
  const hoursToMs = (h) => h * minToMs(60);
  const daysToMs = (d) => d * hoursToMs(24);
  const padStart = (value, length) => {
    let result = makeString(value);
    while (result.length < length) {
      result = '0' + result;
    }
    return result;
  };
  
  const fourYearsInMs = daysToMs(365 * 4 + 1);
  let year = 1970 + Math.floor(timestamp / fourYearsInMs) * 4;
  timestamp = timestamp % fourYearsInMs;

  while (true) {
    let isLeapYear = year % 4 === 0;
    let nextTimestamp = timestamp - daysToMs(isLeapYear ? 366 : 365);
    if (nextTimestamp < 0) {
      break;
    }
    timestamp = nextTimestamp;
    year = year + 1;
  }

  const daysByMonth = year % 4 === 0 ? leapYear : nonLeapYear;

  let month = 0;
  for (let i = 0; i < daysByMonth.length; i++) {
    let msInThisMonth = daysToMs(daysByMonth[i]);
    if (timestamp > msInThisMonth) {
      timestamp = timestamp - msInThisMonth;
    } else {
      month = i + 1;
      break;
    }
  }
  let date = Math.ceil(timestamp / daysToMs(1));
  timestamp = timestamp - daysToMs(date - 1);
  let hours = Math.floor(timestamp / hoursToMs(1));
  timestamp = timestamp - hoursToMs(hours);
  let minutes = Math.floor(timestamp / minToMs(1));
  timestamp = timestamp - minToMs(minutes);
  let sec = Math.floor(timestamp / secToMs(1));
  timestamp = timestamp - secToMs(sec);
  let milliSeconds = timestamp;

  return (
    year +
    '-' +
    padStart(month, 2) +
    '-' +
    padStart(date, 2) +
    'T' +
    padStart(hours, 2) +
    ':' +
    padStart(minutes, 2) +
    ':' +
    padStart(sec, 2) +
    '.' +
    padStart(milliSeconds, 3) +
    'Z'
  );
}


